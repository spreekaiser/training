{
  "name": "project-manager-reactive",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "concurrently \"node projects-server/server.js db=projects-server/db.json\" \"ng serve\" ",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "install-server": "npm install --prefix ./projects-server ./projects-server",
    "cypress": "concurrently \"node projects-server/server.js db=projects-server/test.json\"  \"ng serve\" \"cypress open\" ",
    "cypress:ci": "concurrently \"node projects-server/server.js db=projects-server/test.json\" \"cypress run\" ",
    "postinstall": "npm run install-server",
    "e2e": "concurrently \"node projects-server/server.js db=projects-server/test.json\"  \"ng e2e\"",
    "cypress:open": "concurrently \"node projects-server/server.js db=projects-server/test.json\"  \"ng serve\" \"cypress open\"",
    "cypress:open-mobile": "concurrently \"node projects-server/server.js db=projects-server/test.json\"  \"ng serve\" \"cypress open --config-file=cypress.mobile.json\"",
    "cypress:run": "concurrently \"node projects-server/server.js db=projects-server/test.json\" \"cypress run\" "
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.0.0",
    "@angular/common": "~13.0.0",
    "@angular/compiler": "~13.0.0",
    "@angular/core": "~13.0.0",
    "@angular/forms": "~13.0.0",
    "@angular/platform-browser": "~13.0.0",
    "@angular/platform-browser-dynamic": "~13.0.0",
    "@angular/router": "~13.0.0",
    "rxjs": "~7.4.0",
    "socket.io-client": "^4.3.2",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.0.1",
    "@angular/cli": "~13.0.1",
    "@angular/compiler-cli": "~13.0.0",
    "@cypress/schematic": "^1.6.0",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^12.11.1",
    "concurrently": "^5.3.0",
    "cypress": "8.3.1",
    "jasmine-core": "~3.10.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "typescript": "~4.4.3"
  }
}
