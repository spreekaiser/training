{
  "name": "project-manager-modular",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "concurrently \"node projects-server/server.js db=projects-server/db.json\" \"ng serve\" ",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test:ci": "ng test --watch=false --browsers=ChromeHeadlessNoSandbox",
    "install-server": "npm install --prefix ./projects-server ./projects-server",
    "postinstall": "npm run install-server",
    "docker:build": "docker build -t project-manager .",
    "docker:run": "docker run -p 8091:80 -d --name pjm --env PJM_API_BASE_URL=http://127.0.0.1:3000/api --env  PJM_WEBSOCKET_URL=http://127.0.0.1:3001 project-manager",
    "docker:stop": "docker stop pjm && docker rm pjm",
    "docker:restart": "npm run docker:stop && npm run docker:run"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.0.0",
    "@angular/common": "~13.0.0",
    "@angular/compiler": "~13.0.0",
    "@angular/core": "~13.0.0",
    "@angular/forms": "~13.0.0",
    "@angular/platform-browser": "~13.0.0",
    "@angular/platform-browser-dynamic": "~13.0.0",
    "@angular/router": "~13.0.0",
    "rxjs": "~7.4.0",
    "socket.io-client": "^4.3.2",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.0.1",
    "@angular/cli": "~13.0.1",
    "@angular/compiler-cli": "~13.0.0",
    "@cypress/schematic": "^1.6.0",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^12.11.1",
    "concurrently": "^5.3.0",
    "cypress": "8.3.1",
    "jasmine-core": "~3.10.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "typescript": "~4.4.3"
  }
}
